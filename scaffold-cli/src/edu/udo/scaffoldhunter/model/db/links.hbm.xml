<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.udo.scaffoldhunter.model.db">

	<class name="Link" table="links">
		<id name="id" column="link_id" access="field">
			<generator class="native"/>
		</id>
		<property name="url" type="text" not-null="true"/>
		<property name="title" type="text" not-null="true"/>
	</class>

</hibernate-mapping>

